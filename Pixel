import java.awt.color.*;
public class Pixel {
	
	private String color;

    public Pixel(){
        this.color = color.valueOf("#FFFFFF");
    }

    public Pixel(String color){
        this.color = color;
    }

    public void setPixel(String color){
        this.color = color;
    }


	public String toString() {
	return " Pixel color is: " + this.color;
	}
}
